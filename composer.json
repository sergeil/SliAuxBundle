{
    "name": "sergeil/aux-bundle",
    "description": "Contains base/utility classes for Sli/* bundles.",
    "keywords": [
        "utils"
    ],
    "homepage": "https://github.com/sergeil/SliAuxBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergei Lissovski",
            "email": "sergei.lissovski@gmail.com",
            "homepage": "http://sergei.lissovski.org/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "doctrine/orm": "^2.2",
        "symfony/symfony": "~2.0|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Sli\\AuxBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}